import Z3Simulation;
@Framework("FMI2");
@FrameworkConfig("FMI2","@file: env.json");
{
Math math = load("Math");
FMI2 tankcontroller = load("FMI2", "{8c4e810f-3df3-4a00-8276-176fa3c9f000}", "../../maestro/src/test/resources/watertankcontroller-c.fmu");
FMI2 SingleWatertank = load("FMI2", "{cfc65592-9ece-4563-9705-1581b6e7071c}",  "../../maestro/src/test/resources/singlewatertank-20sim.fmu");
FMI2Component crtlInstance = tankcontroller.instantiate("crtlInstance", false, false);
FMI2Component wtInstance = SingleWatertank.instantiate("wtInstance", false, false);
Logger logger = load("Logger");
FMI2Component components[2]={wtInstance,crtlInstance};

bool global_execution_continue = true;

@Config("{}");
Z3Simulation.expand simulate(components,0.0,10.0);

